extends servicelayout

block styles
  link(href="./stylesheets/secondHand.css" rel='stylesheet')

block content
  div.page-group
    div.page.page-current
      header.bar.bar-nav.back-blue
        a.button.button-link.button-nav.secondHand-back.pull-left.external(href="/")
          span.icon.icon-left
        a.button.button-link.button-nav.secondHand-back.pull-right.external
          span.icon.text-white
            i.iconfont-selfShare &#xe610;
        h1.title.text-white 二手交易
      nav.bar.bar-tab
        a.tab-item.external(href="/secondHand")
          span.icon.index-color
            i.iconfont-indexTabBar &#xe601;
          span.tab-label 主页
        a.tab-item.external(href="/secondHand/post")
          span.icon
            i.iconfont-treeHoleTabsBar &#xe603;
          span.tab-label 发布
        a.tab-item.external(href="/secondHand/self")
          span.icon
            i.iconfont-treeHoleTabsBar &#xe604;
          span.tab-label 个人中心
      div.bar.bar-header-secondary.secondHand-searchbar
        div.searchbar
          a.searchbar-cancel 取消
          div.search-input
            label.icon.icon-search(for="search")
            input#search(type="search", placeholder="搜索感兴趣的话题")  
                       
      div.content.secondHand-content
        div.buttons-tab
          a.tab-link.button.active(href="#tab-index") 首页
          a.tab-link.button(href="#tab-trans") 交通工具
          a.tab-link.button(href="#tab-sport") 运动健身
          a.tab-link.button(href="#tab-ele") 电子产品
          a.tab-link.button(href="#tab-eleEqui") 电器
          a.tab-link.button(href="#tab-beauty") 丽人美妆
          a.tab-link.button(href="#tab-other") 其他
          a.tab-link.button(href="#tab-eleEqui") 电器
        div.content-block
          div.tabs
            div#tab-index.tab.active
              div.content-block
                  div.card.facebook-card
                    div.card-header.no-border
                      div.facebook-avatar
                          img(src="../../images/user-logo.png")
                      div.card-info
                        div
                          span.facebook-name 朱海浩
                          span.page-view 浏览次数：180
                        div.facebook-date
                          span 07-13 17:00 
                          span 来自[青岛大学]
                    div.card-content
                      div.picsWrapper
                          img(src="../../images/book.jpg")
                      div.title-category
                        span.content-title 收藏图书，低价出售
                        span.category 图书器材
                      div.card-content-text 收藏已久的图书，本人非常喜欢，我愿意分享快乐给大家
                    div.card-footer
                      div.icon.position
                        i.iconfont-position &#xe619;
