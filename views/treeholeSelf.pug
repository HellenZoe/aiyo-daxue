extends treeholelayout

block styles
  link(rel="stylesheet" href="../stylesheets/treeholeSelf.css")

block content
  div.page-group
    div.page.page-current
      header.bar.bar-nav.back-blue
        a.button.button-link.button-nav.treehole-back.pull-left.external(href="/treehole")
          span.icon.icon-left.text-white
        h1.title.text-white 个人中心
      div.content
        div.list-block
          ul
            li
              div.item-content
                div.item-inner
                  div.item-title.label 姓名
                  div.item-input
                    input(type="text" placeholder="尊姓大名")
            li
              div.item-content
                div.item-inner
                  div.item-title.label 电话
                  div.item-input
                    input(type="text" placeholder="您的电话")
            li
              div.item-content
                div.item-inner
                  div.item-title.label qq
                  div.item-input
                    input(type="text" placeholder="您的qq")
            li
              div.item-content
                div.item-inner
                  div.item-title.label 学校
                  div.item-input
                    input(type="text" placeholder="您的学校")
            li
              div.item-content
                div.item-inner
                  div.item-title.label 院系
                  div.item-input
                    input(type="text" placeholder="您的院系")
            li
              div.item-content
                div.item-inner
                  div.item-title.label 密码
                  div.item-input
                    input(type="password" placeholder="输入密码")
                      
        div.content-block
          div.row
            div.col-50
              a.button.button-big.button-fill.button-danger(href="#") 取消
            div.col-50
              a.button.button-big.button-fill.button-success(href="#") 提交
block scripts
  //- script(src="../javascripts/treeholeSelf.js")
  
