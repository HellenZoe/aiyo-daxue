block content
  div.page-group
    div(class="page page-current")
      header(class="bar bar-nav back-blue")
        a.button.button-link.button-nav.treehole-back.pull-left.external(href="/")
          span.icon.icon-left.text-white
        a.button.button-link.button-nav.pull-right.external.text-white.finishPost 完成
        h1.title.text-white 个人信息
      
      div.content
        div.user-info
          div.name
            h3 姓名
            input.border-bottom(placeholder="填写姓名")
          div.gender
            h3 性别
            input.border-bottom(placeholder="填写性别")
          div.tel
            h3 电话
            input.border-bottom(placeholder="填写电话")
          div.qq
            h3 qq
            input.border-bottom(placeholder="填写qq")
          div.学校
            h3 school
            input.border-bottom(placeholder="填写学校")
          div.department
            h3 宿舍地址
            input.border-bottom(placeholder="填写宿舍地址")
              
              

            
            
